{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ], 
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": [
        "/getStops",
        "/getStops.php?searchTerm=",
        "/getLoops",
        "/getLoops.php",
        "/getUsers",
        "/getUsers.php",
        "/getBusNumbers.php",
        "/getBusNumbers"
      ],
      "cacheConfig": {
        "strategy?": "freshness",
        "maxSize": 500,
        "maxAge": "30d"
      }
    }
    
  ]
}
